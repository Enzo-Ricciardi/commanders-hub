{
    "hosting": {
        "public": "dist",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [
            {
                "source": "/frontierAuth",
                "function": "frontierAuth"
            },
            {
                "source": "/frontiercallback",
                "function": "frontierCallback"
            },
            {
                "source": "**",
                "destination": "/index.html"
            }
        ]
    },
    "functions": {
        "source": "functions"
    }
}